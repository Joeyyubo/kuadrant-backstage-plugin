{
  "name": "@kuadrant/external-portal-backstage-plugin-frontend",
  "version": "0.0.1-dev",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kuadrant/kuadrant-backstage-plugin.git",
    "directory": "plugins/external-portal"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.esm.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "external-portal",
    "pluginPackages": [
      "@kuadrant/external-portal-backstage-plugin-frontend"
    ]
  },
  "scalprum": {
